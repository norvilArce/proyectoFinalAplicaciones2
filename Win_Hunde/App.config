<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IServicioEmpleado" />
                <binding name="BasicHttpBinding_IServicioPaciente" />
                <binding name="BasicHttpBinding_IServicioUsuario" />
                <binding name="BasicHttpBinding_IServicioProveedor" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_Hunde/ServicioEmpleado/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioEmpleado"
                contract="ProxyEmpleado.IServicioEmpleado" name="BasicHttpBinding_IServicioEmpleado" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_Hunde/ServicioPaciente/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioPaciente"
                contract="ProxyPaciente.IServicioPaciente" name="BasicHttpBinding_IServicioPaciente" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_Hunde/ServicioUsuario/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioUsuario"
                contract="ProxyUsuario.IServicioUsuario" name="BasicHttpBinding_IServicioUsuario" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/WCF_Hunde/ServicioProveedor/"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IServicioProveedor"
                contract="ProxyProveedor1.IServicioProveedor" name="BasicHttpBinding_IServicioProveedor" />
        </client>
    </system.serviceModel>
</configuration>